    "ngx-highlightjs": "^3.0.1",
    "ngx-perfect-scrollbar": "^7.2.0",
    "ngx-quill": "^4.5.0",
    "ngx-restangular": "^4.0.1",
    "ngx-showdown": "^4.0.2",
    "nvd3": "^1.8.6",
    "quill": "^1.3.6",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "screenfull": "^4.0.0",
    "showdown": "^1.9.0",
    "survey-angular": "^1.0.64",
    "surveyjs-editor": "^1.0.64",
    "uuid": "^3.3.2",
    "vued3tree": "^3.6.1",
    "zone.js": "~0.8.28"
  },
