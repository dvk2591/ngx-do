{
  "name": "ngx-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sjhoeksma/ngx-do.git"
  },
  "version": "0.2.1-1",
  "scripts": {
    "ng": "ng",
    "start": "npm run inc_release_demo && npm run build && ng serve ngx-do-demo",
    "test": "ng test ngx-do-demo --code-coverage",
    "test-single": "ng test ngx-do-cdk --watch=false --code-coverage",
    "lint": "ng lint",
    "build_cdk": "ng build --prod ngx-do-cdk && cd projects/ngx-do-cdk && npm run scss_bundle",
    "publish_cdk": "npm run inc_patch_cdk && npm run build && cd dist/ngx-do-cdk && npm publish",
    "inc_patch_cdk": "cd projects/ngx-do-cdk && npm version patch -f",
    "build_do": "cd projects/ngx-do && npm run build",
    "publish_do": "cd projects/ngx-do && npm run publish",
    "build_pages": "cd projects/ngx-do-pages && npm run build",
    "publish_pages": "cd projects/ngx-do-pages && npm run publish",
    "build_widgets": "cd projects/ngx-do-widgets && npm run build",
    "publish_widgets": "cd projects/ngx-do-widgets && npm run publish",
    "build_ai": "cd projects/ngx-do-ai && npm run build",
    "publish_ai": "cd projects/ngx-do-ai && npm run publish",
    "build_survey": "cd projects/ngx-do-survey && npm run build",
    "publish_survey": "cd projects/ngx-do-survey && npm run publish",
    "build_msal": "cd projects/ngx-do-msal && npm run build",
    "publish_msal": "cd projects/ngx-do-msal && npm run publish",
    "build_wip": "cd projects/ngx-do-wip && npm run build",
    "publish_wip": "cd projects/ngx-do-wip && npm run publish",
    "build_all": "npm run build_do && npm run build_pages && npm run build_ai && npm run build_survey && npm run build_widgets && npm run build_wip",
    "publish_all": "npm run publish_do && npm run publish_pages && npm run publish_ai && npm run publish_survey && npm run publish_widgets && npm run publish_wip",
    "build_demo": "npm run inc_patch_demo && ng build ngx-do-demo --",
    "gateway": "cd projects/ngx-do-api-gateway && node index.js",
    "build_gateway": "cd projects/ngx-do-api-gateway && npm run build",
    "publish_gateway": "npm run build_gateway && cd dist/ngx-do-api-gateway && npm publish"
  },
  "private": false,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^7.2.1",
    "@angular/cdk": "^7.2.1",
