{
  "name": "ngx-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sjhoeksma/ngx-do.git"
  },
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "proxy": "cd projects/ngx-do-proxy && node index.js",
    "start": "npm run build && ng serve ngx-do-demo",
    "test": "ng test ngx-do-demo --code-coverage",
    "test-single": "ng test ngx-do-cdk --watch=false --code-coverage",
    "lint": "ng lint",
    "inc_version": "cd projects/ngx-do-cdk && npm version prerelease -f",
    "build": "ng build --prod ngx-do-cdk && cd projects/ngx-do-cdk && npm run scss_bundle",
    "pack": "npm run inc_version && npm run build && cd dist/ngx-do-cdk",
    "publish": "npm run pack && cd dist/ngx-do-cdk && npm publish",
    "inc_version_proxy": "cd projects/ngx-do-proxy && npm version prerelease -f",
    "build_demo": "ng build ngx-do-demo ",
    "build_proxy": "cd projects/ngx-do-proxy && npm run build",
    "pack_proxy": "npm run inc_version_proxy && npm run build_proxy && cd dist/ngx-do-proxy",
    "publish_proxy": "npm run pack_proxy && cd dist/ngx-do-proxy && npm publish"
  },
  "private": false,
