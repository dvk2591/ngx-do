    "@types/node": "^10.12.18",
    "bundle-scss": "^1.3.4",
    "codelyzer": "^4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "jquery": "^3.3.1",
    "ng-packagr": "^4.6.0",
    "tsickle": "^0.34.0",
    "reinstall": "^2.0.0",
    "protractor": "^5.4.2",
    "tslib": "^1.9.0",
    "tslint": "^5.12.1",
    "typescript": "~3.2.4"
  }
}
